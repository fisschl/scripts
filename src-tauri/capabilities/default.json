{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "*"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "store:default",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-stat",
    "fs:allow-copy-file",
    "fs:allow-create",
    "fs:allow-read-dir",
    "fs:allow-exists"
  ]
}
